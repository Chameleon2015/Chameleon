<addons>
<addon id="plugin.program.chameleonbuilds" name="Chameleon Builds" version="1.0.6" provider-name="Chameleon-Builds">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Chameleon-Builds</summary>
    <description lang="en">Easy Way To Install Your Chameleon build!</description>
    <platform>all</platform>
  </extension>
</addon>

