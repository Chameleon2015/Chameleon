		<language></language>
		<forum>http://iptvtalk.org/showthread.php?7379-Modified-IPTV-Stalker-(9-0-18)-iVue-compatible</forum>


	</extension>
</addon>

    <addon id="repository.Chameleon" name="Chameleon Repo" version="1.0.2" provider-name="Chameleon">
